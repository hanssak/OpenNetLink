    public void RecvUrlDatafromBrowser(int groupid, RecvUrlEventArgs e)
    {

        CLog.Here().Information($"groupID : {groupid} - Recv Url from Browser : {e.strUrlData}");

        // 서버에 접속되어 있는지 확인
        if (CanConnectServer(groupid) == false)
